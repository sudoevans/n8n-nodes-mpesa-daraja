{
  "name": "n8n-nodes-mpesa-daraja",
  "version": "0.1.7",
  "description": "n8n community node for Safaricom M-Pesa Daraja 3.0 API - Mobile payments, STK Push, C2B, B2C, B2B transactions",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "mpesa",
    "safaricom",
    "daraja",
    "payments",
    "mobile-money",
    "kenya",
    "stk-push",
    "c2b",
    "b2c",
    "b2b"
  ],
  "license": "MIT",
  "author": {
    "name": "Evans",
    "email": "snaveqiptoo@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudoevans/n8n-nodes-mpesa-daraja.git"
  },
  "homepage": "https://github.com/sudoevans/n8n-nodes-mpesa-daraja#readme",
  "bugs": {
    "url": "https://github.com/sudoevans/n8n-nodes-mpesa-daraja/issues"
  },
  "main": "index.js",
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.{ts,js,md}\"",
    "lint": "eslint nodes credentials --ext .ts --fix",
    "lintcheck": "eslint nodes credentials --ext .ts",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Mpesa/Mpesa.node.js",
      "dist/nodes/MpesaTrigger/MpesaTrigger.node.js"
    ],
    "credentials": [
      "dist/credentials/MpesaApi.credentials.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.30.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "~5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
